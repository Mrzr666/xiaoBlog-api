<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xblog.modules.leavemessage.mapper.LeavemessageMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xblog.modules.leavemessage.entity.Leavemessage">
        <id column="id" property="id" />
        <result column="leave_message" property="leaveMessage" />
        <result column="stars" property="stars" />
        <result column="created_date" property="createdDate" />
        <result column="deleted" property="deleted" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, leave_message, stars, created_date, deleted
    </sql>

</mapper>
